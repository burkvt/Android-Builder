[
  {
    "kernelSource": {
      "name": "XiaomiNote3-jason",
      "repo": "https://github.com/crdroidandroid/android_kernel_xiaomi_jason",
      "branch": "15.0",
      "device": "jason",
      "defconfig": "jason_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-vortex",
        "branch": "15.0",
        "name": "clang", 
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://github.com/crdroidandroid/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-gnu-9.3",
        "branch": "15.0",
        "name": "aarch64",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "LD_LIBRARY_PATH": "/home/runner/work/Android-Builder/Android-Builder/clang/lib",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": "",
        "CFLAGS": "--sysroot=/home/runner/work/Android-Builder/Android-Builder/aarch64 --gcc-toolchain=/home/runner/work/Android-Builder/Android-Builder/aarch64 -no-integrated-as"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]

[
  {
    "kernelSource": {
      "name": "XiaomiNote3-Jason",
      "repo": "https://github.com/burkvt/android_kernel_xiaomi_sdm660",
      "branch": "lineage-21-4.19",
      "device": "jason",
      "defconfig": "jason_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-gnu-9.3",
        "branch": "lineage-21.0",
        "name": "aarch64",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]
